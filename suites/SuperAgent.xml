	<test name="Cash Out">
		<classes>
			<class name="portals.testCases.Agent.CashOut">
				<methods>
					<include name="cashOut" />
					<include name="verifyPerformedCashOutFromFromUserPassbook" />
					<include name="verifyPerformedCashOutFromToUserPassbook" />
					<include name="cashOutFromDashboard" />
					<include name="verifyPerformedCashOutFromDashboardFromUserPassbook" />
					<include name="verifyPerformedCashOutFromDashboardFromToUserPassbook" />
					<include name="cashOutFromSideMenu" />
					<include name="verifyPerformedCashOutFromSideMenuFromUserPassbook" />
					<include name="verifyPerformedCashOutFromSideMenuFromToUserPassbook" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Cash In">
		<classes>
			<class name="portals.testCases.Agent.CashInByAgent">
				<methods>
					<include name="cashInByAgent" />
					<include name="verifyPerformedCashInByAgentFromFromUserPassbook" />
					<include name="verifyPerformedCashInByAgentFromToUserPassbook" />
					<include name="cashInByAgentFromSideMenu" />
					<include name="verifyPerformedCashInByAgentFromSideMenuFromUserPassbook" />
					<include
						name="verifyPerformedCashInByAgentFromSideMenuFromToUserPassbook" />
					<include name="cashInByAgentFromSend" />
					<include name="verifyPerformedCashInByAgentFromSendFromUserPassbook" />
					<include name="verifyPerformedCashInByAgentFromSendFromToUserPassbook" />
					<include name="cashInByAgentFromDashboard" />
					<include
						name="verifyPerformedCashInByAgentFromDashboardFromUserPassbook" />
					<include
						name="verifyPerformedCashInByAgentFromDashboardFromToUserPassbook" />
				</methods>
			</class>
		</classes>
	</test>
