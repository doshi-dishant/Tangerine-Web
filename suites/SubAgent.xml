<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- <?xml version="1.0" encoding="UTF-8"?> -->

<suite name="Tangereine SubAgent WebPortal - Sub Agent"
	verbose="10" parallel="false" thread-count="5">

	<listeners>
		<listener class-name='listeners.CustomizedEmailableReport' />
		<listener class-name='listeners.HtmlReport' />
	</listeners>

	<parameter name="browserType" value="chrome" />
	<test name="Sub Agent - Common Testcases">
		<classes>
			<class name="portals.testCases.Common.CommonTestcasesSubAgent">
				<methods>
					<include name="qrCodeFromDashboard" />
					<include name="passBookFromDashboard" />
					<include name="verifyDownloadPassbook" />
					<include name="mpinPolicyFromDashboardFooter" />

					<include name="checkBalanceFromDashboardViewBalance" />
					<include name="recentTransactionFromDashboard" />

					<include name="privacyPolicyFromDashboardFooter" />
					<include name="privacyPolicyFromSideMenuSettings" />
					<include name="termsAndConditionsFromSideMenuSettings" />
					<include name="termsAndConditionsFromDashboardFooter" />
					<include name="mpinPolicyFromChangeMpincreenFromMyProfile" />
					<include name="passwordPolicyChangePasswordFromMyProfile" />
					<include
						name="passwordPolicyChangePasswordScreenFromSideMenuSettings" />
					<include name="passwordPolicyFromDashboardFooter" />
					<include name="FAQsFromSideMenu" />
					<include name="CheckBalanceFromMyProfile" />

					<include name="logout" />

				</methods>
			</class>
		</classes>
	</test>

	<test name="Sub Agent - Change MPIN">
		<classes>
			<class name="portals.testCases.SubAgent.ChangeMpin">
				<methods>
					<include name="changeMpinFromSideMenu" />
					<include name="changeMpinFromMyProfileSubAgent" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="Sub Agent - Change Password">
		<classes>
			<class name="portals.testCases.SubAgent.ChangePassword">
				<methods>
					<include name="changePasswordFromSideMenu" />
					<include name="changePasswordFromMyProfileSubAgent" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="Sub Agent - Change Secret Word">
		<classes>
			<class name="portals.testCases.SubAgent.ChangeSecretWord">
				<methods>
					<include name="changeSecretWordSubAgent" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="WithdrawCash and SubAgent Assisted Withdraw">
		<classes>
			<class
				name="portals.testCases.SubAgent.SubAgentAssistedWithdrawCash">
				<methods>
					<include
						name="withdrawCashAndAgentAssistedWithdrawfromDashboard" />
					<include
						name="verifyPerformedAssistedWithDrawFromDashboardToUserPassbook" />
					<include
						name="verifyPerformedWithDrawCashtFromDashboardFromUserPassbook" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="WithdrawCash and Agent Assisted Withdraw from Footer">
		<classes>
			<class
				name="portals.testCases.SubAgent.SubAgentAssistedWithdrawCash">
				<methods>

					<include
						name="withdrawCashAndAgentAssistedWithdrawfromFooter" />
					<include
						name="verifyPerformedAssistedWithDrawFromFooterToUserPassbook" />
					<include
						name="verifyPerformedWithDrawCashtFromFooterFromUserPassbook" />

				</methods>
			</class>
		</classes>
	</test>

	<test
		name="WithdrawCash and Agent Assisted Withdraw from Side Menu">
		<classes>
			<class
				name="portals.testCases.SubAgent.SubAgentAssistedWithdrawCash">
				<methods>
					<include
						name="withdrawCashAndAgentAssistedWithdrawfromSideMenu" />
					<include
						name="verifyPerformedAssistedWithDrawFromSideMenuToUserPassbook" />
					<include
						name="verifyPerformedWithDrawCashtFromSideMenuFromUserPassbook" />

				</methods>
			</class>
		</classes>
	</test>


</suite>